DECLARE @IDEMPLEADO INT
DECLARE @proyecto_id  int 
DECLARE  @motnoprovedio numeric(9,2)

SET @IDEMPLEADO =(SELECT  empleado_id FROM RH.Empleado_Proyecto where codigo = 'vlh00141')
SET @proyecto_id =(SELECT  proyecto_id  FROM RH.Empleado_Proyecto where codigo = 'vlh00141')



exec pl.Promedio12Vaca @IDEMPLEADO, @PROYECTO_id,  @motnoprovedio OUT
select @motnoprovedio
